{
    "name": "MyBookmarks BySiDiMaD",
    "description": "Backup And View Your Bookmarks ByS!D!M@D",
    "version": "1.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "bookmarks",
        "storage",
        "activeTab",
        "scripting"
    ],
    "options_page": "about.html",
    "action": {
        "default_popup": "index.html"
  },
    "icons": {
        "16": "/icons/icon-16.png",
        "24": "/icons/icon-24.png",
        "32": "/icons/icon-32.png",
        "64": "/icons/icon-64.png",
        "128": "/icons/icon-24.png"
        
    }
}